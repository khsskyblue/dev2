                },
                ".content-block": {
                  "opacity": "1",
                  "transform": "translateY(-50%)"
                }
              }
            },
            ".carousel-item": {
              "-webkit-justify-content": "center",
              "justify-content": "center"
            },
            ".carousel-item.active, .carousel-item-next, .carousel-item-prev": {
              "display": "flex"
            },
            ".carousel-indicators": {
              "margin": "20px auto 0",
              "align-items": "center",
              "bottom": "-40px",
              ".active": {
                "transform": "scale(1)",
                "border": "0",
                "background-color": "fade(@activeIndicatorColor, 80%)",
                "transition": "all 200ms ease-in-out"
              },
              "li": {
                "height": "10px",
                "width": "10px",
                "border-radius": "50%",
                "transition": "opacity 0.5s",
