        {
          "alias": false,
          "_styles": {
            ".mbr-section": {
              "text-align": "center",
              "color": "#fff"
            },
            "p": {
              "display": "inline",
              "font-size": "unit(@sizetext, px)",
              "color": "#fff",
              "& when (@showneon)": {
                "& when (@effectstyle = 'stroke')": {
                  "text-shadow": "-1px -1px 0 @textshadow, 1px -1px 0 @textshadow, -1px 1px 0 @textshadow, 1px 1px 0 @textshadow, 1px 1px 10px #141414"
                },
                "& when (@effectstyle = 'neon')": {
                  "text-shadow": "0 0 2px @textshadow, 0 0 30px @textshadow, 0px 0px 5px @textshadow, 0 0 150px @textshadow"
                },
                "& when (@effectstyle = 'offset')": {
                  "text-shadow": "4px 4px 0px @textshadow, 7px 7px 0px rgba(0, 0, 0, 0.2)"
                },
                "& when (@effectstyle = '3d')": {
                  "text-shadow": "@textshadow 3px 5px 0, @dcolor1 4px 6px 3px, @dcolor2 8px 11px 10px"
                }
              }
            },
            "& when (@Style = 'defile')": {
              ".marquee-rtl": {
                "width": "unit(@wdtext, %)",
                "max-width": "100%",
                "overflow": "hidden"
              },
              ".marquee-rtl > :first-child": {
                "display": "inline-block",
                "padding-right": "2em",
                "padding-left": "100%",
                "white-space": "nowrap",
                "animation": "defilement-rtl (@speedtext * 1s) infinite linear",
                "animation-iteration-count": "(@counttext * 1)"
              },
              "& when (@moussehover)": {
                ".marquee-rtl:hover :first-child": {
                  "animation-play-state": "paused"
                }
              },
              "@keyframes defilement-rtl": {
                "0%": {
                  "transform": "translate3d(0, 0, 0)"
                },
                "100%": {
                  "transform": "translate3d(-100%, 0, 0)"
                }
              },
              ".media-container-row .mbr-text": {
                "color": "#ffffff"
              }
            },
            "& when (@Style = 'blink')": {
              ".marquee-rtl": {
                "animation": "Test (@speedtext * 1s) infinite",
                "padding-right": "unit(@rightpad, %)",
                "padding-left": "unit(@leftpad, %)",
                "animation-iteration-count": "(@counttext * 1)"
              },
              "@keyframes Test": {
                "0%": {
                  "opacity": "1"
                },
                "50%": {
                  "opacity": "0"
                },
                "100%": {
                  "opacity": "1"
                }
              }
            },
            "& when (@Style = 'fix')": {
              ".marquee-rtl": {
                "padding-right": "unit(@rightpad, %)",
                "padding-left": "unit(@leftpad, %)"
              }
            },
            "& when (@Style = 'pulse')": {
              ".marquee-rtl": {
                "-webkit-animation": "Test (@speedtext * 1s) ease-out",
                "-webkit-animation-iteration-count": "infinite",
                "opacity": "0.5",
                "padding-right": "unit(@rightpad, %)",
                "padding-left": "unit(@leftpad, %)"
              },
              "@-webkit-keyframes Test": {
                "0%": {
                  "opacity": "0.5"
                },
                "50%": {
                  "opacity": "1.0"
                },
                "100%": {
                  "opacity": "0.5"
                }
              }
            },
            ".navbar": {
              "padding": "0.5rem 0",
              "background": "@menuBgColor",
              "transition": "none",
              "min-height": "50px"
            },
            ".navbar-fixed-top": {
              "padding-top": "0"
            },
            ".navbar-collapse": {
              "-ms-flex-preferred-size": "auto",
              "-webkit-flex-basis": "auto",
              "flex-basis": "auto"
            },
            ".menu-logo": {
              ".navbar-brand": {
                "display": "flex",
                "margin-left": "1.5rem",
                "padding": "0.5rem 0",
                "transition": "padding 0.2s",
                "min-height": "3.8rem",
                "-webkit-align-items": "center",
                "align-items": "center",
                ".navbar-caption-wrap": {
                  "display": "flex",
                  "-webkit-align-items": "center",
                  "align-items": "center",
