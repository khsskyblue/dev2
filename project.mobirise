            ".carousel": {
              "margin-top": "56px"
            },
            ".wrapper": {
              "position": "relative",
              "padding": "2rem",
              "border-radius": "4px",
              "display": "flex",
              "-webkit-justify-content": "center",
              "justify-content": "center",
              "-webkit-align-items": "flex-end",
              "align-items": "flex-end",
              "height": "~\"@{cardHeight}rem\"",
              "margin-bottom": "30px",
              ".mbr-overlay": {
                "background": "@cardOverlayColor",
                "opacity": "0",
                "z-index": "1",
                "transition": "all 0.4s ease",
                "border-radius": "4px"
              },
              "img": {
                "position": "absolute",
                "top": "0",
                "left": "0",
                "width": "100%",
                "height": "100%",
                "object-fit": "cover",
                "object-position": "center center",
                "border-radius": "4px"
              },
              ".content-block": {
                "position": "absolute",
                "width": "100%",
                "z-index": "1",
                "top": "50%",
                "left": "0",
                "transform": "translateY(-80%)",
                "opacity": "0",
                "padding": "0.75rem 1rem",
                "transition": "all 0.4s ease"
              },
              "&:hover": {
                ".mbr-overlay": {
                  "background": "@cardOverlayColor",
                  "opacity": "@cardOpacity",
